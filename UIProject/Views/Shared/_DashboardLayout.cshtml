<!DOCTYPE html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Hızlı Analiz</title>
    <!-- plugins:css -->
    <link rel="stylesheet" href="~/new-dashboard/template/assets/vendors/mdi/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="~/new-dashboard/template/assets/vendors/css/vendor.bundle.base.css">
    <!-- endinject -->
    <!-- Plugin css for this page -->
    <link rel="stylesheet" href="~/new-dashboard/template/assets/vendors/jvectormap/jquery-jvectormap.css">
    <link rel="stylesheet" href="~/new-dashboard/template/assets/vendors/flag-icon-css/css/flag-icon.min.css">
    <link rel="stylesheet" href="~/new-dashboard/template/assets/vendors/owl-carousel-2/owl.carousel.min.css">
    <link rel="stylesheet" href="~/new-dashboard/template/assets/vendors/owl-carousel-2/owl.theme.default.min.css">
    <link rel="stylesheet" href="~/new-dashboard/template/assets/vendors/select2/select2.min.css">
    <link rel="stylesheet" href="~/new-dashboard/template/assets/vendors/select2-bootstrap-theme/select2-bootstrap.min.css">
    <!-- End plugin css for this page -->
    <!-- inject:css -->
    <!-- endinject -->
    <!-- Layout styles -->
    @if ((@ViewBag.DarkTheme == false))
    {
            <link rel="stylesheet" href="~/new-dashboard/template/assets/css/style2.css">
    }
    else
    {
            <link rel="stylesheet" href="~/new-dashboard/template/assets/css/style.css">
    }

    <!-- End layout styles -->
    <link rel="shortcut icon" href="~/new-dashboard/template/assets/images/hizli_analiz_favicon-01.png" />
  </head>
  <body>
    <div class="container-scroller">
      <!-- partial:~/new-dashboard/template/partials/_sidebar.html -->
      <nav class="sidebar sidebar-offcanvas" id="sidebar">
        <div class="sidebar-brand-wrapper d-none d-lg-flex align-items-center justify-content-center fixed-top">
          
                @if(@ViewBag.DarkTheme == false)
                {
                <a class="sidebar-brand brand-logo" href="/Dashboard/"><img src="~/new-dashboard/template/assets/images/hizli_analiz_logo2_siyah.png" alt="logo" /></a>
                <a class="sidebar-brand brand-logo-mini" href="/Dashboard/"><img src="~/new-dashboard/template/assets/images/hizli_analiz_logo2_siyah.png" alt="logo" /></a>
                }
                else{
                    <a class="sidebar-brand brand-logo" href="/Dashboard/"><img src="~/new-dashboard/template/assets/images/hizli_analiz_logo2.png" alt="logo" /></a>
                <a class="sidebar-brand brand-logo-mini" href="/Dashboard/"><img src="~/new-dashboard/template/assets/images/hizli_analiz_logo2.png" alt="logo" /></a>
                }
        </div>
        <ul class="nav">
          <li class="nav-item profile">
            <div class="profile-desc">
              <div class="profile-pic">
                <div class="count-indicator">
                    @if(@ViewBag.Picture != null)
                    {
                    <img class="img-xs rounded-circle " src="~/userImages/profiles/@ViewBag.Picture" alt="">
                                    
                    }else {
                    <img class="img-xs rounded-circle " src="~/new-dashboard/template/assets/images/faces-clipart/pic-1.png" alt="">
                    }
                <span class="count bg-success"></span>
                </div>
                <div class="profile-name">
                  <h5 class="mb-0 font-weight-normal">@ViewBag.username</h5>
                @if(@ViewBag.isAdmin == true)
                {
                    <span>Admin</span>
                }
                </div>
              </div>
              <a href="#" id="profile-dropdown" data-toggle="dropdown"><i class="mdi mdi-dots-vertical"></i></a>
              <div class="dropdown-menu dropdown-menu-right sidebar-dropdown preview-list" aria-labelledby="profile-dropdown">
                <a href="/Profile/" class="dropdown-item preview-item">
                  <div class="preview-thumbnail">
                    <div class="preview-icon bg-dark rounded-circle">
                      <i class="mdi mdi-settings text-primary"></i>
                    </div>
                  </div>
                  <div class="preview-item-content">
                    <p class="preview-subject ellipsis mb-1 text-small">Ayarlar</p>
                  </div>
                </a>
                <div class="dropdown-divider"></div>
                <a href="/Profile/ChangePassword" class="dropdown-item preview-item">
                  <div class="preview-thumbnail">
                    <div class="preview-icon bg-dark rounded-circle">
                      <i class="mdi mdi-onepassword  text-info"></i>
                    </div>
                  </div>
                  <div class="preview-item-content">
                    <p class="preview-subject ellipsis mb-1 text-small">Şifre Değiştir</p>
                  </div>
                </a>
                <div class="dropdown-divider"></div>
                <a href="#" class="dropdown-item preview-item">
                  <div class="preview-thumbnail">
                    <div class="preview-icon bg-dark rounded-circle">
                      <i class="mdi mdi-calendar-today text-success"></i>
                    </div>
                  </div>
                  <div class="preview-item-content">
                    <p class="preview-subject ellipsis mb-1 text-small">Yapılacaklar Listesi</p>
                  </div>
                </a>
              </div>
            </div>
          </li>
          <li class="nav-item nav-category">
            <span class="nav-link">ARACLAR</span>
          </li>
          <li class="nav-item menu-items">
            <a class="nav-link" href="/Dashboard/">
              <span class="menu-icon">
                <i class="mdi mdi-speedometer"></i>
              </span>
              <span class="menu-title">Dashboard</span>
            </a>
          </li>
          <li class="nav-item menu-items">
            <a class="nav-link" href="/Commission/">
              <span class="menu-icon">
                <i class="mdi mdi-speedometer"></i>
              </span>
              <span class="menu-title">Komisyon Hesapla</span>
            </a>
          </li>
          <li class="nav-item menu-items">
            <a class="nav-link" data-toggle="collapse" href="#ui-basic" aria-expanded="false" aria-controls="ui-basic">
              <span class="menu-icon">
                <i class="mdi mdi-laptop"></i>
              </span>
              <span class="menu-title">Takip Araçları</span>
              <i class="menu-arrow"></i>
            </a>
            <div class="collapse" id="ui-basic">
              <ul class="nav flex-column sub-menu">
                <li class="nav-item"> <a class="nav-link" href="/RankFinder/">Sıralama Bulucu</a></li>
                <li class="nav-item"> <a class="nav-link" href="/PriceTrack/">Fiyat Takibi</a></li>
                <li class="nav-item"> <a class="nav-link" href="/BuyBoxTracker/">BuyBox Takibi</a></li>
              </ul>
            </div>
          </li>
          <li class="nav-item menu-items">
            <a class="nav-link" href="/MarketAnalysis/">
              <span class="menu-icon">
                <i class="mdi mdi-playlist-play"></i>
              </span>
              <span class="menu-title">Piyasa Analizi</span>
            </a>
          </li>
          <li class="nav-item menu-items">
            <a class="nav-link" href="/SearchProduct/">
              <span class="menu-icon">
                <i class="mdi mdi-table-large"></i>
              </span>
              <span class="menu-title">Ürün Araştırma</span>
            </a>
          </li>
          <li class="nav-item nav-category">
            <span class="nav-link">API</span>
          </li>
          <li class="nav-item menu-items">
            <a class="nav-link" data-toggle="collapse" href="#ui-ent" aria-expanded="false" aria-controls="ui-ent">
              <span class="menu-icon">
                <i class="mdi mdi-laptop"></i>
              </span>
              <span class="menu-title">Entegrasyon</span>
              <i class="menu-arrow"></i>
            </a>
            <div class="collapse" id="ui-ent">
              <ul class="nav flex-column sub-menu">
                <li class="nav-item"> <a class="nav-link" href="/API/">API Bilgilerim</a></li>
                <li class="nav-item"> <a class="nav-link" href="/UserProducts/">Ürünlerim</a></li>
                <li class="nav-item"> <a class="nav-link" href="/Orders/">Siparişlerim</a></li>
              </ul>
            </div>
          </li>
          @if (@ViewBag.isAdmin == true)
                {
          <li class="nav-item nav-category">
            <span class="nav-link">GUVENLIK</span>
          </li>
          
          <li class="nav-item menu-items">
            <a class="nav-link" data-toggle="collapse" href="#auth" aria-expanded="false" aria-controls="auth">
              <span class="menu-icon">
                <i class="mdi mdi-security"></i>
              </span>
                
                    <span class="menu-title">Admin</span>
                
                <i class="menu-arrow"></i>
            </a>
            <div class="collapse" id="auth">
              <ul class="nav flex-column sub-menu">
                <li class="nav-item"> <a class="nav-link" href="~/new-dashboard/template/pages/samples/error-404.html"> Ürünler </a></li>
                <li class="nav-item"> <a class="nav-link" href="~/new-dashboard/template/pages/samples/error-500.html"> Kategoriler </a></li>
                <li class="nav-item"> <a class="nav-link" href="~/new-dashboard/template/pages/samples/login.html"> Müşteriler </a></li>
                <li class="nav-item"> <a class="nav-link" href="~/new-dashboard/template/pages/samples/register.html"> Personeller </a></li>
              </ul>
            </div>
          </li>
          }
        </ul>
      </nav>
      <!-- partial -->
      <div class="container-fluid page-body-wrapper">
        <!-- partial:~/new-dashboard/template/partials/_navbar.html -->
        <nav class="navbar p-0 fixed-top d-flex flex-row">
          <div class="navbar-brand-wrapper d-flex d-lg-none align-items-center justify-content-center">
            <a class="navbar-brand brand-logo-mini" href="~/new-dashboard/template/index.html"><img src="~/new-dashboard/template/assets/images/logo-mini.svg" alt="logo" /></a>
          </div>
          <div class="navbar-menu-wrapper flex-grow d-flex align-items-stretch">
            <button class="navbar-toggler navbar-toggler align-self-center" type="button" data-toggle="minimize">
              <span class="mdi mdi-menu"></span>
            </button>
            <ul class="navbar-nav w-100">
              <li class="nav-item w-100">
                <form class="nav-link mt-2 mt-md-0 d-none d-lg-flex search">
                  <input type="text" class="form-control" placeholder="Arama...">
                </form>
              </li>
            </ul>
            <ul class="navbar-nav navbar-nav-right">
              <li class="nav-item nav-settings d-none d-lg-block">
                <a class="nav-link" href="/Dashboard/ChangeTheme">
                  <i class="mdi mdi-theme-light-dark"></i>
                </a>
              </li>
              <li class="nav-item dropdown border-left">
                <a class="nav-link count-indicator dropdown-toggle" id="messageDropdown" href="#" data-toggle="dropdown" aria-expanded="false">
                  <i class="mdi mdi-email"></i>
                    @if(@ViewBag.MessageCount != 0)
                    {
                        <span class="count bg-success"></span>
                    }     
                </a>
                <div class="dropdown-menu dropdown-menu-right navbar-dropdown preview-list" aria-labelledby="messageDropdown">
                  <h6 class="p-3 mb-0">Mesajlar</h6>
                  <div class="dropdown-divider"></div>
                  <!--
                  <a class="dropdown-item preview-item">
                    <div class="preview-thumbnail">
                      <img src="@ViewBag.Picture" alt="image" class="rounded-circle profile-pic">
                    </div>
                    <div class="preview-item-content">
                      <p class="preview-subject ellipsis mb-1">Mesaj</p>
                      <p class="text-muted mb-0"> 1 Dakika önce </p>
                    </div>
                  </a>
                  -->
                  <p class="p-3 mb-0 text-center">@ViewBag.MessageCount yeni mesaj</p>
                </div>
              </li>
              <li class="nav-item dropdown border-left">
                <a class="nav-link count-indicator dropdown-toggle" id="notificationDropdown" href="#" data-toggle="dropdown">
                  <i class="mdi mdi-bell"></i>
                    @if(@ViewBag.alertcount != 0)
                    {
                        <span class="count bg-danger"></span>
                    }       
                </a>
                <div class="dropdown-menu dropdown-menu-right navbar-dropdown preview-list" aria-labelledby="notificationDropdown">
                  <h6 class="p-3 mb-0">Notifications</h6>
                  @await Component.InvokeAsync("AlertList")
                  <div class="dropdown-divider"></div>
                  <p class="p-3 mb-0 text-center">@ViewBag.alertcount yeni bildirim</p>
                </div>
              </li>
              <li class="nav-item dropdown">
                <a class="nav-link" id="profileDropdown" href="#" data-toggle="dropdown">
                  <div class="navbar-profile">
                    @if(@ViewBag.Picture != null)
                    {
                    <img class="img-xs rounded-circle " src="~/userImages/profiles/@ViewBag.Picture" alt="">
                                    
                    }else {
                    <img class="img-xs rounded-circle " src="~/new-dashboard/template/assets/images/faces-clipart/pic-1.png" alt="">
                    }
                    <p class="mb-0 d-none d-sm-block navbar-profile-name">@ViewBag.username</p>
                    <i class="mdi mdi-menu-down d-none d-sm-block"></i>
                  </div>
                </a>
                <div class="dropdown-menu dropdown-menu-right navbar-dropdown preview-list" aria-labelledby="profileDropdown">
                  <h6 class="p-3 mb-0">Profil</h6>
                  <div class="dropdown-divider"></div>
                  <a href="/Profile/" class="dropdown-item preview-item">
                    <div class="preview-thumbnail">
                      <div class="preview-icon bg-dark rounded-circle">
                        <i class="mdi mdi-settings text-success"></i>
                      </div>
                    </div>
                    <div class="preview-item-content">
                      <p class="preview-subject mb-1">Ayarlar</p>
                    </div>
                  </a>
                  <div class="dropdown-divider"></div>
                  <a href="/Login/Logout" class="dropdown-item preview-item">
                    <div class="preview-thumbnail">
                      <div class="preview-icon bg-dark rounded-circle">
                        <i class="mdi mdi-logout text-danger"></i>
                      </div>
                    </div>
                    <div class="preview-item-content">
                      <p class="preview-subject mb-1">Çıkış yap</p>
                    </div>
                  </a>
                </div>
              </li>
            </ul>
            <button class="navbar-toggler navbar-toggler-right d-lg-none align-self-center" type="button" data-toggle="offcanvas">
              <span class="mdi mdi-format-line-spacing"></span>
            </button>
          </div>
        </nav>
        <!-- partial -->
        <div class="main-panel">
          <div class="content-wrapper">
                    @RenderBody()
                    @await RenderSectionAsync("Scripts",false)
          </div>
          <footer class="footer">
            <div class="d-sm-flex justify-content-center justify-content-sm-between">
              <span class="text-muted d-block text-center text-sm-left d-sm-inline-block">Copyright © hizlianaliz.com 2022</span>
            </div>
          </footer>
          <!-- partial -->
        </div>
        <!-- main-panel ends -->
      </div>
      <!-- page-body-wrapper ends -->
    </div>
    <!-- container-scroller -->
    <!-- plugins:js -->
    <script src="~/new-dashboard/template/assets/vendors/js/vendor.bundle.base.js"></script>
    <script src="~/new-dashboard/template/assets/vendors/select2/select2.min.js"></script>
    <!-- endinject -->
    <!-- Plugin js for this page -->
    <script src="~/new-dashboard/template/assets/vendors/chart.js/Chart.min.js"></script>
    <script src="~/new-dashboard/template/assets/vendors/progressbar.js/progressbar.min.js"></script>
    <script src="~/new-dashboard/template/assets/vendors/jvectormap/jquery-jvectormap.min.js"></script>
    <script src="~/new-dashboard/template/assets/vendors/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <script src="~/new-dashboard/template/assets/vendors/owl-carousel-2/owl.carousel.min.js"></script>
    <!-- End plugin js for this page -->
    <!-- inject:js -->
    <script src="~/new-dashboard/template/assets/js/off-canvas.js"></script>
    <script src="~/new-dashboard/template/assets/js/hoverable-collapse.js"></script>
    <script src="~/new-dashboard/template/assets/js/file-upload.js"></script>
    <script src="~/new-dashboard/template/assets/js/misc.js"></script>
    <script src="~/new-dashboard/template/assets/js/settings.js"></script>
    <script src="~/new-dashboard/template/assets/js/todolist.js"></script>
    <script src="~/new-dashboard/template/assets/js/select2.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/2.0.1/js/toastr.js"></script>
    <link href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/2.0.1/css/toastr.css" rel="stylesheet"/>  
    <!-- endinject -->
    <!-- Custom js for this page -->
    @if ((@ViewBag.DarkTheme == true))
    {
            <script src="~/new-dashboard/template/assets/js/dashboard2.js"></script>
    }
    else
    {
            <script src="~/new-dashboard/template/assets/js/dashboard.js"></script>
    }

    <!-- End custom js for this page -->
  </body>
</html>